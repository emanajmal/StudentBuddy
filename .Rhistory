observeEvent(input$assign_match, {
if (!is.null(match_result()) && input$student_name != "") {
insert_student_query <- paste0("INSERT INTO students (name, email, country, language, program, arrival_date) VALUES ('",
input$student_name, "', 'unknown@university.edu', '",
input$student_country, "', 'unknown', '",
input$student_program, "', CURRENT_DATE)")
dbExecute(con, insert_student_query)
sid_query <- paste0("SELECT student_id FROM students WHERE name = '", input$student_name, "' ORDER BY student_id DESC LIMIT 1")
sid <- dbGetQuery(con, sid_query)$student_id[1]
bid <- match_result()$buddy_id
insert_match_query <- paste0("INSERT INTO matches (student_id, buddy_id, status) VALUES (",
sid, ", ", bid, ", 'Matched')")
dbExecute(con, insert_match_query)
showModal(modalDialog("Student and Match saved successfully!", easyClose = TRUE))
}
})
